{
  "manifest_version": 2,
  "name": "Chrome Extension TypeScript Starter",
  "description": "Chrome Extension, TypeScript, React",
  "version": "1.0",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "<all_urls>"
  ],
  "web_accessible_resources": [ "*.mp3" ]
}
