{
  "manifest_version": 2,
  "name": "Safu",
  "description": "Keep track of addresses and private keys, the safu way",
  "version": "0.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": ["*.mp3"],
  "content_security_policy": "script-src 'self' 'unsafe-eval';"
}
